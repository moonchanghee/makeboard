            <label class="control-label" for="id">아이디</label>
            <input class="form-control" type="text" name="userId" id="id"/>
            <span class="glyphicon glyphicon-ok form-control-feedback"></span>
        </div>

        <div class="form-group has-feedback">
            <label class="control-label" for="id">비밀번호</label>
            <input class="form-control" type="password" name="userPwd" id="id"/>
            <span class="glyphicon glyphicon-ok form-control-feedback"></span>
        </div>  
        <button class="btn btn-success" type="submit">로그인</button>
        </form>
        <button class="btn btn-success" type="submit" id="back">나가기</button>
    </div>

    <script>
$('#back').click(function(){
    history.back();
});
    </script>
